{
  "name": "nocvp/zf3-strings",
  "type": "module",
  "keywords": [
    "cms",
    "zf3",
    "strings"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager-di": "^1.0",
    "zendframework/zend-modulemanager": "^2.7",
    "zendframework/zend-view": "^2.8"
  },
  "config": {
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "Strings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "StringsTest\\": "test/"
    }
  }
}
